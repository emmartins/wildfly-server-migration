                            <tool-distributionType>standalone</tool-distributionType>
                        </attributes>
                    </configuration>
                </plugin>
                <plugin>
	                <groupId>org.apache.maven.plugins</groupId>
    	            <artifactId>maven-dependency-plugin</artifactId>
    	            <configuration>
                        <excludes>META-INF/**</excludes>
                    </configuration>
                </plugin>
            </plugins>
        </pluginManagement>
